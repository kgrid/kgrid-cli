{
  "name": "@kgrid/cli",
  "description": "Command-line tool for Knowledge Object Developers (Note: Compatible with the latest KO structure)",
  "version": "0.4.4",
  "author": "kgrid",
  "bin": {
    "kgrid": "./bin/run"
  },
  "bugs": "https://github.com/kgrid/kgrid-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "archiver": "^3.0.0",
    "axios": "^0.19.2",
    "cli-ux": "^5.4.4",
    "colors": "^1.3.3",
    "download": "^7.1.0",
    "form-data": "^2.5.0",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.5.0",
    "js-yaml": "^3.13.1",
    "jsonpath": "^1.0.2",
    "klaw-sync": "^6.0.0",
    "listr": "^0.14.3",
    "request": "^2.88.2",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "tmp": "^0.1.0",
    "vuepress": "^1.3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/kgrid/kgrid-cli",
  "keywords": [
    "oclif",
    "kgrid"
  ],
  "license": "GPL-3.0-or-later",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "kgrid",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./src/hooks/init/checkversion"
    }
  },
  "repository": "kgrid/kgrid-cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && npm run oclif-readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "oclif-readme": "(cd docs && oclif-dev readme)",
    "version": "npm run oclif-readme && git add docs/README.md",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  }
}
